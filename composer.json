{
    "name": "spryker/install",
    "type": "library",
    "description": "Install module",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "spryker/kernel": "^3.30.0",
        "spryker/log": "^3.0.0",
        "spryker/monolog": "^2.0.0",
        "spryker/symfony": "^3.5.0"
    },
    "require-dev": {
        "codeception/module-asserts": "^1.3.0 || ^3.0.0",
        "phpstan/phpstan": "1.9.4",
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "stan": "vendor/bin/phpstan analyse -l 5 -c phpstan.neon src/",
        "stan-lowest": "vendor/bin/phpstan analyse -l 5 -c phpstan-lowest.neon src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "use-include-path": true,
        "process-timeout": 600,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "bin": [
        "bin/install"
    ]
}
