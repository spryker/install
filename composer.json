{
  "name": "spryker/install",
  "description": "Spryker install tool",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "symfony/console": "^3.0.0 || ^4.0.0",
    "symfony/process": "^3.0.0 || ^4.0.0",
    "symfony/yaml": "^3.0.0 || ^4.0.0",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "symfony/var-dumper": "*",
    "phpstan/phpstan": "^0.11.2",
    "spryker/code-sniffer": "*",
    "php-coveralls/php-coveralls": "^1.0",
    "phpunit/phpunit": "<8"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "use-include-path": true,
    "process-timeout": 600
  },
  "bin": [
    "bin/install"
  ],
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p src/ tests/",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p src/ tests/"
  }
}
